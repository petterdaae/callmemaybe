{
    "patterns": [
        {
            "match": "'(([^\\\\'])|(\\\\')|(\\\\\\\\))'",
            "name": "string.quoted.single.ts"
        },
        {
            "match": "\\\"(([^\\\\'])|(\\\\')|(\\\\\\\\))*\\\"",
            "name": "string.quoted.double.ts"
        },
        {
            "match": "true",
            "name": "constant.language.boolean.true.ts"
        },
        {
            "match": "false",
            "name": "constant.language.boolean.false.ts"
        },
        {
            "match": "return|if|loop",
            "name": "keyword.control.flow.ts"
        },
        {
            "match": "println|struct",
            "name": "entity.name.function.ts"
        },
        {
            "match": "bool|int|char|list|func|string",
            "name": "support.type.primitive.ts"
        },
        {
            "match": "[a-zA-Z_][a-zA-Z0-9_]*",
            "name": "variable.other.readwrite.ts"
        },
        {
            "match": "[0-9]+",
            "name": "constant.numeric.decimal.ts"
        },
        {
            "include": "#expression"
        }
    ],
    "scopeName": "source.cmm"
}