input = <int, 6>[1721, 979, 366, 299, 675, 1456]

solve = | me, input list<int, 6>, i int, j int | list<int, 2> {
    if i < 0 {
        return <int, 2>[-1, -1]
    }
    if j < 0 {
        return <int, 2>[-1, -1]
    }
    x = get i from input
    y = get j from input
    if x + y == 2020 {
        return <int, 2>[i, j]
    }
    tuple = call me with input, i - 1, j
    if (get 0 from tuple) > 0 {
        return tuple
    }
    return call me with input, i, j - 1
}

tuple = call solve with input, 5, 5

a = get get 0 from tuple from input
b = get get 1 from tuple from input

println a * b
