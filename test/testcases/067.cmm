input = <int, 6>[1721, 979, 366, 299, 675, 1456]
solve = | me, input list<int>, i int, j int | int {
    if i < 0 {
        return -1
    }
    x = get i from input
    y = get j from input
    if x + y == 2020 {
        return x * y
    }
    sol = #me(input, i - 1, j)
    if sol > 0 {
        return sol
    }
    return #me(input, i, j - 1)
}

solution = #solve(input, 5, 5)

println solution
